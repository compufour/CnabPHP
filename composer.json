{
  "name": "zucchetti-pos/cnab-php",
  "type": "library",
  "description": "Projeto para criar arquivos de remessas e processar arquivos de retorno no formato CNAB",
  "keywords": [
    "boleto",
    "remessa",
    "retorno"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Anderson Danilo",
      "email": "andersondanilo.souza@gmail.com"
    },
    {
      "name": "Hermenegildo Marin Junior",
      "email": "hmarinjr@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "phpunit/php-code-coverage": "1.2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "require": {
    "php": ">=5.4.0",
    "zucchetti-pos/cnab-yaml": "dev-main",
    "mustangostang/spyc": "0.5.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/zucchetti-pos/cnab-yaml"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cnab\\": "src/Cnab"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
